<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>

    <class name="model.StudentAnswer" table="student_answer">
    
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        
        <property name="studentTestId" column="student_test_id" type="integer" />
        
        <property name="questionId" column="question_id" type="integer" />
         
        <property name="answerId" column="answer_id" type="integer" />
        
    </class>
    
</hibernate-mapping>
